            </setting>
            <setting name="UniqueIdentifier" serializeAs="String">
                <value />
            </setting>
            <setting name="GamesWon" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="GamesLost" serializeAs="String">
                <value>1</value>
            </setting>
